# The compiler
FC = gfortran
# flags for debugging or for maximum performance, comment as necessary
FCFLAGS = -O3 -Wall
PROGRAM main2D
$(FC) $(FCFLAGS) -o $@ $^ $(LDFLAGS)
%.o: %.f
	$(FC) $(FCFLAGS) -c $<

